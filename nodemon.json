{
  "restartable": "rs",
  "verbose": false,
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  },
  "execMap": {
    "": "node",
    "js": "node"
  },
  "watch": [
    "app/",
    "bin/",
    "app.js"
  ],
  "ignore": [
    ".git",
    ".idea",
    "node_modules"
  ],
  "ext": "js html"
}